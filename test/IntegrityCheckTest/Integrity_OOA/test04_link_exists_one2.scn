private service Integrity::test04_link_exists_one2() is
l1: instance of List;
e1: instance of Element;
begin

  // Relate the same instance twice across a 'one' relationship

  l1 := create List(name => "TODO");

  e1 := create Element(list_name => l1.name, item => "Finish videos");
  link e1 R1 l1;
  link e1 R2 l1;
  link e1 R2 l1;  // Accidentally link the instance twice

end service;
